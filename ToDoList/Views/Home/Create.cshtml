@model ToDoList.Models.MyToDoList
@using ToDoList.Models
<!DOCTYPE html>



<environment include="Development">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</environment>
<meta charset="utf-8" />

<html>
<head>
    <title>Customer Form</title>
</head>
<body>
    <form method="post">
        <fieldset>
            <legend>Add Tasc</legend>
            <div>
                <label for="DateTime">Date Time:</label>
                <input type="text" name="DateTime" value="" />
            </div>
            <div>
                <label for="Tasc">Tasc:</label>
                <input type="text" name="Tasc" value="" />
            </div>
            <div>
                <label for="importance">Importance:</label>
                <input type="text" name="importance" value="" />
            </div>
            <div>
                <label for="CompletedNoCompleted">CompletedNoCompleted:</label>
                <input type="text" name="CompletedNoCompleted" value="" />
            </div>
            <div>
                <label>&nbsp;</label>
                <input type="submit" value="Submit" class="submit" />
            </div>
        </fieldset>
    </form>
</body>
</html>


